<% @d.ds_elements.each do |element| %>
<p>
  <%= label_tag element.name %>
  <% if element.type == "File" %>
  	<%= @record[element.key] %>
  <% elsif element.type == "Image"%>
  	<%= @record[element.key] %>
  	<%#= image_tag @record[element.key].url if @record[element.key] %>
  <% else %>
  	<%= @record[element.key] %>
  <% end %>
</p>
<% end %>